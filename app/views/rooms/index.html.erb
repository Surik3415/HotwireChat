<%= turbo_stream_from :rooms %>
<%= turbo_frame_tag :rooms do %>
  <%= render @rooms%>
<% end%>
<% if current_user %>
  <%= form_with model: @new_room do |f|%>

    <div class="group relative flex justify-center rounded-md bg-indigo-600 py-2 px-3 text-sm font-semibold text-white hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600 w-40 mt-5">
    <%= f.submit "Create new room" %>
    </div>

  <%end%>
<%end%>
